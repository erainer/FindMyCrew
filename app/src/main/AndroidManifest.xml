<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidbootcamp.findmycrew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccount" />
        <activity android:name=".BuyTickets" />
        <activity android:name=".SellTickets" />
        <activity android:name=".FindYourCrew" />
        <activity android:name=".Explore" />
        <activity android:name=".FriendsFeed" />
        <activity android:name=".Profile" />
        <activity android:name=".YourFeed" />
        <activity android:name=".ForgotAccount" />
        <activity android:name=".SpringAwakening"></activity>
    </application>

</manifest>